{
  "name": "@dimerapp/markdown",
  "version": "6.1.0",
  "description": "A markdown process",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json dimerapp/markdown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimerapp/markdown.git"
  },
  "keywords": [
    "markdown",
    "remark",
    "ast",
    "dimerapp"
  ],
  "author": "virk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimerapp/markdown/issues"
  },
  "homepage": "https://github.com/dimerapp/markdown#readme",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^3.0.0",
    "@adonisjs/require-ts": "^2.0.3",
    "@types/node": "^14.14.37",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^3.0.1",
    "doctoc": "^2.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-adonis": "^1.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "github-label-sync": "^2.0.0",
    "husky": "^5.2.0",
    "japa": "^3.1.1",
    "mrm": "^2.6.1",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "ts-dedent": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@types/hast": "^2.3.1",
    "@types/mdast": "^3.0.4",
    "@types/unist": "^2.0.3",
    "front-matter": "^4.0.2",
    "hast-util-to-html": "^7.1.2",
    "hast-util-to-string": "^1.0.4",
    "hastscript": "^6.0.0",
    "mdast-util-to-hast": "^10.2.0",
    "mdast-util-toc": "^5.1.0",
    "parseurl": "^1.3.3",
    "rehype-raw": "^5.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-directive": "^1.0.1",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-slug": "^6.0.0",
    "remark-squeeze-paragraphs": "^4.0.0",
    "remark-stringify": "^9.0.1",
    "unified": "^9.2.1",
    "unist-util-visit": "^2.0.3",
    "vfile-message": "^2.0.4"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  }
}
